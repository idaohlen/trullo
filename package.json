{
  "name": "trullo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx watch server/index.ts",
    "build": "vue-tsc -b && vite build && npm run build:server",
    "build:client": "vue-tsc -b && vite build",
    "build:server": "tsc -p tsconfig.server.json && tsc-alias -p tsconfig.server.json",
    "start": "NODE_ENV=production node dist-server/index.js",
    "start:dev": "tsx server/index.ts",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "clean": "rm -rf dist dist-server"
  },
  "dependencies": {
    "@apollo/client": "^3.7.15",
    "@apollo/server": "^5.0.0",
    "@as-integrations/express5": "^1.1.2",
    "@tailwindcss/vite": "^4.1.13",
    "@vue/apollo-composable": "^4.2.2",
    "@vueuse/core": "^13.9.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "graphql": "^16.11.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-vue-next": "^0.543.0",
    "mongoose": "^8.18.1",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "zod": "^4.1.7"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
